<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exam.k_pop">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".StartApplication.ApplicationStart"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name=".Achievements" android:screenOrientation="portrait"></activity>
        <activity android:name=".TwoBandsTinder"  android:screenOrientation="portrait"/>
        <activity android:name=".gallery.activity.GalleryBioPage"  android:screenOrientation="portrait"/>
        <activity android:name=".Settings"  android:screenOrientation="portrait"/>
        <activity android:name=".GuessStar"  android:screenOrientation="portrait"/>
        <activity android:name=".GuessBands"  android:screenOrientation="portrait"/>
        <activity android:name=".gallery.activity.Gallery"  android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>