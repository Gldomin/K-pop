<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exam.k_pop">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".StartApplication.ApplicationStart"
        android:theme="@style/AppTheme">
        <activity android:name=".gallery.activity.GalleryBioPage"/>
        <activity android:name=".Settings" />
        <activity android:name=".GuessStar" />
        <activity android:name=".GuessBands" />
        <activity android:name=".gallery.activity.Gallery" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.exam.k_pop.GuesGroup">
        </activity>
    </application>

</manifest>